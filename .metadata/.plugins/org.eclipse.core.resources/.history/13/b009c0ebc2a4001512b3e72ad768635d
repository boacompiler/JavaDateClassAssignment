import java.awt.*;
import java.awt.event.ActionListener;
import java.awt.event.*;
import javax.swing.*;

public class Main extends JApplet
{

	DateManager myDateManager;
	public void init()
	{
		myDateManager = new DateManager();
		
		Frame table = new Frame();
		table.setBounds(100,100,300,300);
		//TODO table.isVisible();
		
		double size[][] =
            {{0.25, 0.25, 0.25, 0.25},
             {50, TableLayout.FILL, 40, 40, 40}};
		
		table.setLayout (new TableLayout(size));
	}
	
}
